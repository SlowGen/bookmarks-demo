{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "lib": ["DOM", "ESNext", "DOM.Iterable"],
    "outDir": "web",
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "alwaysStrict": true,
    "strictPropertyInitialization": true,
    "strictBindCallApply": true,
    "pretty": true,
    "noImplicitAny": true
  }
}
